input centoblecent;
skalo:=.8;

% piktogramo malka≈ùi, t.e. angulo montranta malsupren

picture pic;
pic := image (

    pickup pencircle scaled 10;
    draw (-50,100)--(50,100);
    draw (-50,85)--(50,85);
    draw (-50,70)--(50,70);
    draw (-40,10)--(0,50)--(40,10);

    % evitu shovadon malsupren per nevidebla baz-linio...
    pickup pencircle scaled 0;
    draw (-50,0)--(50,0);
);

coc_svg(pic,skalo);
coc_png(pic,skalo);

end.