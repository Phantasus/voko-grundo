package de.steloj.respiro;

import java.text.RulesBasedCollator;

public class CatalanCollator extends RulesBasedCollator {

    static final String CatalanSortRules = " = '|' < a,A,à,À < b,B < c,C,ç,Ç < d,D < e,E,é,É,è,È < f,F < g,G < h,H < i,I,í,Í,ï,Ï < j,J < k,K < l,L,l·,L· < m,M < n,N < o,O,ó,Ó,ò,Ò < p,P < q,Q < r,R < s,S < t,T < u,U,ú,Ú,ü,Ü < v,V < w,W < x,X < y,Y < z,Z";

    public CatalanCollator() {
	super(CatalanSortRules);
    }
}
